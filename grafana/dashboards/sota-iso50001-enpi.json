      "targets": [
        {
          "datasource": {
            "type": "postgres",
            "uid": "TimescaleDB"
          },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT \n  s.name as \"SEU Name\",\n  s.description as \"Description\",\n  ROUND(SUM(sep.actual_consumption)::numeric, 0) as \"Total Energy (kWh)\",\n  ROUND(AVG(sep.deviation_percent)::numeric, 1) as \"Deviation %\",\n  ROUND(SUM(sep.expected_consumption - sep.actual_consumption)::numeric, 0) as \"Savings (kWh)\",\n  ROUND((SUM(sep.actual_consumption) / NULLIF(SUM(sep.expected_consumption), 0))::numeric, 3) as \"EnPI\",\n  MAX(sep.compliance_status) as \"Compliance\",\n  TO_CHAR(MAX(sep.period_end), 'Mon DD') as \"Latest Period\"\nFROM seu_energy_performance sep\nJOIN seus s ON sep.seu_id = s.id\nWHERE sep.period_start >= '2025-07-01'::date\nGROUP BY s.name, s.description\nORDER BY SUM(sep.actual_consumption) DESC\nLIMIT 10;",
          "refId": "A"
        }
      ],
