          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT \n  m.name as \"Machine\",\n  m.type as \"Type\",\n  ROUND(SUM(er.total_energy_kwh)::numeric, 2) as \"Energy (kWh)\",\n  ROUND(AVG(er.avg_power_kw)::numeric, 2) as \"Avg Power (kW)\",\n  ROUND(MAX(er.peak_demand_kw)::numeric, 2) as \"Peak (kW)\",\n  COUNT(*) as \"Readings\"\nFROM energy_readings_1min er\nJOIN machines m ON er.machine_id = m.id\nWHERE $__timeFilter(bucket)\n  AND m.id IN ($machines)\nGROUP BY m.name, m.type\nORDER BY SUM(er.total_energy_kwh) DESC;",
          "refId": "A",
          "sql": {
