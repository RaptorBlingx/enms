        {
          "datasource": {
            "type": "postgres",
            "uid": "TimescaleDB"
          },
          "editorMode": "code",
          "format": "time_series",
          "rawQuery": true,
          "rawSql": "-- Forecasts\nSELECT \n  forecast_time as time,\n  'Forecast' as metric,\n  AVG(predicted_power_kw) as value\nFROM energy_forecasts\nWHERE forecast_time >= $__timeFrom()\n  AND forecast_time <= $__timeTo()\nGROUP BY forecast_time\n\nUNION ALL\n\n-- Upper Bound\nSELECT \n  forecast_time as time,\n  'Upper Bound' as metric,\n  AVG(upper_bound_kw) as value\nFROM energy_forecasts\nWHERE forecast_time >= $__timeFrom()\n  AND forecast_time <= $__timeTo()\n  AND upper_bound_kw IS NOT NULL\nGROUP BY forecast_time\n\nUNION ALL\n\n-- Lower Bound\nSELECT \n  forecast_time as time,\n  'Lower Bound' as metric,\n  AVG(lower_bound_kw) as value\nFROM energy_forecasts\nWHERE forecast_time >= $__timeFrom()\n  AND forecast_time <= $__timeTo()\n  AND lower_bound_kw IS NOT NULL\nGROUP BY forecast_time\n\nORDER BY time, metric;",
          "refId": "A"
        },
        {
          "datasource": {
            "type": "postgres",
            "uid": "TimescaleDB"
          },
          "editorMode": "code",
          "format": "time_series",
          "hide": false,
          "rawQuery": true,
          "rawSql": "-- Actual power (from energy readings aggregates)\nSELECT \n  bucket as time,\n  'Actual Power' as metric,\n  AVG(avg_power_kw) as value\nFROM energy_readings_1min\nWHERE bucket >= $__timeFrom()\n  AND bucket <= $__timeTo()\nGROUP BY bucket\nORDER BY bucket;",
          "refId": "B"
        }
      ],
      "title": "Power Forecast vs Actual (24-Hour)",
      "type": "timeseries"
    },
