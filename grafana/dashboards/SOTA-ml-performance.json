      "targets": [
        {
          "datasource": {
            "type": "postgres",
            "uid": "TimescaleDB"
          },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT \n  m.name as \"Machine\",\n  mth.model_type as \"Model Type\",\n  mth.model_version as \"Version\",\n  mth.training_status as \"Status\",\n  TO_CHAR(mth.training_start, 'Mon DD HH24:MI') as \"Started\",\n  EXTRACT(EPOCH FROM (mth.training_end - mth.training_start))/60 as \"Duration (min)\",\n  mth.trigger_type as \"Trigger\",\n  ROUND(mth.performance_improvement, 2) as \"Improvement\",\n  mth.sample_count as \"Samples\"\nFROM model_training_history mth\nJOIN machines m ON mth.machine_id = m.id\nWHERE mth.training_start >= NOW() - INTERVAL '30 days'\nORDER BY mth.training_start DESC\nLIMIT 10;",
          "refId": "A"
        }
      ],
      "title": "Recent Model Training History (30d)",
