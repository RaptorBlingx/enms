          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "WITH current AS (\n  SELECT \n    SUM(total_energy_kwh) as energy,\n    SUM(total_energy_kwh * 0.12) as cost,\n    SUM(total_energy_kwh * 0.45) as co2\n  FROM energy_readings_1day\n  WHERE bucket >= DATE_TRUNC('month', NOW())\n),\nlast AS (\n  SELECT \n    SUM(total_energy_kwh) as energy,\n    SUM(total_energy_kwh * 0.12) as cost,\n    SUM(total_energy_kwh * 0.45) as co2\n  FROM energy_readings_1day\n  WHERE bucket >= DATE_TRUNC('month', NOW()) - INTERVAL '1 month'\n    AND bucket < DATE_TRUNC('month', NOW())\n)\nSELECT \n  'Energy Consumption' as \"Metric\",\n  ROUND(c.energy::numeric, 0)::text || ' kWh' as \"Current\",\n  CASE \n    WHEN c.energy > l.energy THEN 'â†‘ ' || ROUND(((c.energy - l.energy) / l.energy * 100)::numeric, 1)::text || '%'\n    WHEN c.energy < l.energy THEN 'â†“ ' || ROUND(((l.energy - c.energy) / l.energy * 100)::numeric, 1)::text || '%'\n    ELSE 'â†’ 0%'\n  END as \"Trend\"\nFROM current c, last l\n\nUNION ALL\n\nSELECT \n  'Energy Cost',\n  '$' || ROUND(c.cost::numeric, 0)::text,\n  CASE \n    WHEN c.cost > l.cost THEN 'â†‘ ' || ROUND(((c.cost - l.cost) / l.cost * 100)::numeric, 1)::text || '%'\n    WHEN c.cost < l.cost THEN 'â†“ ' || ROUND(((l.cost - c.cost) / l.cost * 100)::numeric, 1)::text || '%'\n    ELSE 'â†’ 0%'\n  END\nFROM current c, last l\n\nUNION ALL\n\nSELECT \n  'CO2 Emissions',\n  ROUND(c.co2::numeric, 0)::text || ' kg',\n  CASE \n    WHEN c.co2 > l.co2 THEN 'â†‘ ' || ROUND(((c.co2 - l.co2) / l.co2 * 100)::numeric, 1)::text || '%'\n    WHEN c.co2 < l.co2 THEN 'â†“ ' || ROUND(((l.co2 - c.co2) / l.co2 * 100)::numeric, 1)::text || '%'\n    ELSE 'â†’ 0%'\n  END\nFROM current c, last l;",
          "refId": "A"
        }
      ],
      "title": "Executive Summary (Month-over-Month)",
