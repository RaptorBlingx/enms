          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT \n  'Unresolved Anomalies' as \"Concern\",\n  COUNT(*)::text as \"Count\",\n  CASE \n    WHEN COUNT(*) >= 5 THEN 'High'\n    WHEN COUNT(*) >= 2 THEN 'Medium'\n    ELSE 'Low'\n  END as \"Status\"\nFROM anomalies\nWHERE resolved_at IS NULL\n\nUNION ALL\n\nSELECT \n  'Non-Compliant SEUs' as \"Concern\",\n  COUNT(DISTINCT seu_id)::text as \"Count\",\n  CASE \n    WHEN COUNT(DISTINCT seu_id) >= 5 THEN 'High'\n    WHEN COUNT(DISTINCT seu_id) >= 2 THEN 'Medium'\n    ELSE 'Low'\n  END as \"Status\"\nFROM seu_energy_performance\nWHERE compliance_status != 'compliant'\n  AND period_start >= '2025-09-01'::date\n\nUNION ALL\n\nSELECT \n  'Machines Below Target' as \"Concern\",\n  COUNT(*)::text as \"Count\",\n  CASE \n    WHEN COUNT(*) >= 4 THEN 'High'\n    WHEN COUNT(*) >= 2 THEN 'Medium'\n    ELSE 'Low'\n  END as \"Status\"\nFROM machine_status\nWHERE health_score < 90\n\nORDER BY 3 DESC, 2 DESC\nLIMIT 3;",
          "refId": "A"
        }
      ],
