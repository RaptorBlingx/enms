          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT \n  m.name as \"Machine\",\n  a.anomaly_type as \"Type\",\n  a.severity as \"Severity\",\n  COALESCE(ROUND(a.metric_value::numeric, 2), 0) as \"Value\",\n  a.detected_at as \"Detected At\",\n  ROUND(EXTRACT(EPOCH FROM (NOW() - a.detected_at))/3600, 1) as \"Hours Ago\"\nFROM anomalies a\nJOIN machines m ON a.machine_id = m.id\nJOIN factories f ON m.factory_id = f.id\nWHERE a.is_resolved = false\n  AND f.name IN ($factory)\nORDER BY a.detected_at DESC\nLIMIT 10;",
          "refId": "A"
        }
