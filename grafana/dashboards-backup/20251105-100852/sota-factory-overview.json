      "targets": [
        {
          "datasource": {
            "type": "postgres",
            "uid": "TimescaleDB"
          },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "WITH latest_readings AS (\n  SELECT DISTINCT ON (machine_id)\n    machine_id,\n    avg_power_kw,\n    bucket\n  FROM energy_readings_1min\n  ORDER BY machine_id, bucket DESC\n),\nmachine_anomalies AS (\n  SELECT \n    machine_id,\n    COUNT(*) as anomaly_count\n  FROM anomalies\n  WHERE is_resolved = false\n  GROUP BY machine_id\n)\nSELECT \n  m.name as \"Machine\",\n  CASE WHEN m.is_active THEN 'Active' ELSE 'Inactive' END as \"Status\",\n  COALESCE(lr.avg_power_kw, 0) as \"Current Power\",\n  COALESCE(ma.anomaly_count, 0) as \"Anomalies\",\n  ROUND(\n    CASE \n      WHEN COALESCE(ma.anomaly_count, 0) = 0 THEN 95\n      WHEN COALESCE(ma.anomaly_count, 0) <= 5 THEN 85\n      WHEN COALESCE(ma.anomaly_count, 0) <= 10 THEN 70\n      ELSE 50\n    END\n  ) as \"Health Score\"\nFROM machines m\nJOIN factories f ON m.factory_id = f.id\nLEFT JOIN latest_readings lr ON m.id = lr.machine_id\nLEFT JOIN machine_anomalies ma ON m.id = ma.machine_id\nWHERE m.is_active = true\n  AND f.name IN ($factory)\nORDER BY \"Health Score\" ASC, \"Anomalies\" DESC;",
          "refId": "A"
        }
      ],
