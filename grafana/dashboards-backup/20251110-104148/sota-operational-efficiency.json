      "pluginVersion": "10.2.0",
      "targets": [
        {
          "datasource": {
            "type": "postgres",
            "uid": "TimescaleDB"
          },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "WITH oee_calc AS (\n  SELECT \n    COUNT(*) FILTER (WHERE ms.is_running) * 100.0 / NULLIF(COUNT(*), 0) as availability,\n    AVG(pd.production_count) * 100.0 / NULLIF(MAX(pd.production_count), 0) as performance,\n    100.0 as quality\n  FROM machine_status ms\n  CROSS JOIN LATERAL (\n    SELECT production_count \n    FROM production_data \n    WHERE time >= NOW() - INTERVAL '24 hours'\n  ) pd\n)\nSELECT ROUND((availability * performance * quality / 10000)::numeric, 1) as \"OEE\"\nFROM oee_calc;",
          "refId": "A"
        }
      ],
      "title": "Overall Equipment Effectiveness (OEE)",
      "type": "gauge"
    },
