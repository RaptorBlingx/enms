          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT \n  m.name as \"Machine\",\n  COUNT(*) as \"Total\",\n  COUNT(CASE WHEN a.severity = 'critical' THEN 1 END) as \"Critical\",\n  COUNT(CASE WHEN a.severity = 'warning' THEN 1 END) as \"Warning\",\n  COUNT(CASE WHEN a.is_resolved = false THEN 1 END) as \"Unresolved\",\n  TO_CHAR(MAX(a.detected_at), 'Mon DD HH24:MI') as \"Latest\"\nFROM anomalies a\nJOIN machines m ON a.machine_id = m.id\nWHERE a.detected_at >= $__timeFrom()\n  AND a.detected_at <= $__timeTo()\nGROUP BY m.name\nORDER BY COUNT(*) DESC;",
          "refId": "A",
